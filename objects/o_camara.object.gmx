        if vigilado != -1
        {
            xx = vigilado.x;
            yy = vigilado.y - 108;
        }
    }
    //movimiento camara
    var vel = clamp(point_distance(x, y, xx, yy) / 400000, 0, 0.0009);
    direction = point_direction(x, y, xx, yy);
    x += lengthdir_x(delta_time * vel, direction);
    y += lengthdir_y(delta_time * vel, direction);
    depth = -y;
}

//animacion de la helice y subeybaja
