            <string>//animacion
anima += delta_time;
if anima &gt;= 100000
{
    anima -= 100000;
    //pies
    paso++;
    if paso &gt; 3
    {
        paso = 0;
    }
    //cabeza
    if subex
    {
        anix += random(3);
        if anix &gt; 4.5
        {
            subex = false;
        }
    }
    else
    {
        anix -= random(3);
        if anix &lt; -4.5
        {
            subex = true;
        }
    }
    if subey
    {
        aniy += random(3);
        if aniy &gt; 9
        {
            subey = false;
        }
    }
    else
    {
        aniy -= random(3);
        if aniy &lt; -4.5
        {
            subey = true;
        }
    }
}

//desaparecer y moverse
if !o_game.pausa
{
    if !hablar
    {
        reloj -= delta_time;
    }
    if reloj &lt;= 0
    {
        instance_destroy();
    }
    else
    {
        var otro = instance_place(x, y, oo_bloque);
        if otro != noone
        {
            direction = point_direction(otro.x, otro.y, x, y);
            x += lengthdir_x(delta_time * m_velrebote, direction);
            y += lengthdir_y(delta_time * m_velrebote, direction);
        }
        else
        {
            otro = instance_place(x, y, o_player);
            if otro != noone
            {
                direction = point_direction(otro.x, otro.y, x, y);
                x += lengthdir_x(delta_time * m_velrebote * 0.75, direction);
                y += lengthdir_y(delta_time * m_velrebote * 0.75, direction);
            }
            else
            {
                otro = instance_place(x, y, oo_enemy);
                if otro != noone
                {
                    direction = point_direction(otro.x, otro.y, x, y);
                    x += lengthdir_x(delta_time * m_velrebote * 0.5, direction);
                    y += lengthdir_y(delta_time * m_velrebote * 0.5, direction);
                }
            }
        }
        x = clamp(x, 144, room_width - 144);
        y = clamp(y, 432, room_height - 144);
        depth = -y;
    }
    
    //mostrar mensaje
    if !hablar and collision_circle(x, y, m_maxfoco * 0.5, o_player, true, true)
    {
        hablar = true;
        audio_play_sound_at(a_mmm, x, y, 0, ma_ref, ma_max, ma_fac, false, 30);
    }
    else if hablar and !collision_circle(x, y, m_vision, o_player, true, true)
    {
        hablar = false;
    }
}
